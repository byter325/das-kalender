    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.1.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "express-xml-bodyparser": "^0.3.0",
    "fast-xml-parser": "^4.0.8",
    "ical": "^0.8.0",
    "node-cron": "^3.0.1",
    "node-ical": "^0.15.1",
    "passport": "^0.6.0",
    "saxon-js": "^2.4.0",
    "swagger-ui-express": "^4.4.0",
    "tmp": "^0.2.1",
    "ts-node": "^10.8.1",
    "xml-js": "^1.6.11",
    "yaml": "^2.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/download": "^8.0.1",
    "@types/express": "^4.17.13",
    "@types/ical": "^0.8.0",
    "@types/node": "^17.0.36",
    "@types/node-cron": "^3.0.2",
    "@types/tmp": "^0.2.3",
    "typescript": "^4.7.2",
    "xslt3": "^2.4.0"
  }
